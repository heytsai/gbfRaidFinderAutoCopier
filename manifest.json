{
  "name": "GBF Raid Finder Auto-copier",
  "description": "Extension that helps auto-copy the newest room ID in the raid finder webpage.",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "webNavigation",
    "clipboardWrite"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://gbf-raidfinder.la-foret.me/",
        "https://gbf-raidfinder-tw.herokuapp.com/"
      ],
      "js": [
        "lib/jquery-3.5.1.js",
        "content.js"
      ]
    }
  ]
}